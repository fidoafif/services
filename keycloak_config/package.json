{
  "name": "@agora/keycloak",
  "version": "0.0.1",
  "description": "Agora Keycloak service",
  "author": "Sofyan Hadi Ahmad",
  "private": true,
  "publishConfig": {
    "access": "private"
  },
  "scripts": {
    "start": "nodemon",
    "start:prod": "node dist/main.js",
    "local": "yarn build && sls offline --port 3003 --skipCacheInvalidation --stage local --env local",
    "offline": "yarn local",
    "build": "rm -rf dist && tsc --p tsconfig.json -d",
    "build:deploy": "yarn build && sls deploy",
    "deploy": "sls deploy",
    "deploy:staging": "NODE_ENV=staging sls deploy --stage staging --env staging",
    "deploy:production": "NODE_ENV=production sls deploy --stage production --env production",
    "barrels": "barrelsby --delete -d ./src --singleQuotes",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:barrels": "barrelsby --delete -d ./src/migrations --singleQuotes",
    "precommit": "yarn e2e:coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json \"src/**/*.ts\" \"e2e/**/*.ts\" -e \"src/migrations/*.ts\" \"*spec.ts\"",
    "lint:fix": "yarn lint --fix",
    "e2e": "jest --config=jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:watch": "jest --watch --config=jest-e2e.json --detectOpenHandles",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage --detectOpenHandles"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@magishift/crud": "^0.0.26",
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "@agora-edu/auth": "^0.0.3",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "json2csv": "^4.5.2",
    "jsonwebtoken": "^8.5.1",
    "keycloak-admin": "^1.12.0",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "pluralize": "^8.0.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.9",
    "@types/uuid": "^3.4.5",
    "barrelsby": "^2.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "serverless-domain-manager": "^3.2.6",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.9.0",
    "serverless-plugin-typescript": "1.1.7",
    "serverless-pseudo-parameters": "^2.4.0",
    "sqlite3": "^4.0.9",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.7",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
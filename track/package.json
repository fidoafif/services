{
  "name": "@agoraenterprise/track",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "deploy": "yarn build && sls deploy",
    "sls:offline": "sls offline --port 3001 --skipCacheInvalidation --env local",
    "start": "ts-node src/main.ts",
    "start:webpack": "node dist/server",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:barrels": "barrelsby --delete -d ./src/database/migrations --singleQuotes",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "e2e:coverage": "jest --config=e2e/jest-e2e.json --coverage --coverageDirectory=./coverage",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "6.2.4",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "6.1.1",
    "@nestjsx/crud": "^4.1.0",
    "@nestjsx/crud-request": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.1.0",
    "@agora-edu/auth": "^0.0.3",
    "aws-lambda": "^1.0.5",
    "aws-serverless-express": "^3.3.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.0.0",
    "jwt-simple": "^0.5.6",
    "mysql": "2.17.1",
    "rxjs": "6.5.2",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.5.0",
    "serverless-plugin-optimize": "^4.1.0-rc.1",
    "serverless-plugin-typescript": "1.1.7",
    "slugify": "^1.3.4",
    "swagger-ui-express": "^4.0.6",
    "typeorm": "0.2.17"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.1",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "7.10.6",
    "barrelsby": "^2.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "serverless-domain-manager": "^3.2.6",
    "supertest": "^4.0.2",
    "ts-loader": "^6.0.3",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  }
}
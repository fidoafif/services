  "description": "description",
  "author": "",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "sls:offline": "sls offline --port 3001 --skipCacheInvalidation --env local",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "e2e": "jest --config=jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=jest-e2e.json",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage"
  },
  "dependencies": {
    "@magishift/config": "^0.0.4",
    "@magishift/http": "^0.0.4",
    "@magishift/redis": "^0.0.3",
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/typeorm": "^6.1.0",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.5",
    "express": "^4.16.4",
    "midtrans-client": "^1.0.3",
    "mysql": "^2.17.1",
    "pg": "^7.10.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.6",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.130",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^4.10.5",
    "serverless-plugin-optimize": "^4.0.2-rc.1",
    "supertest": "^3.4.1",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.16.0",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
{
  "name": "@agora-edu/payment",
  "version": "0.0.0",
  "description": "description",
  "author": "Gustao Chardea",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "deploy": "yarn build && sls deploy",
    "sls:offline": "sls offline --port 3000 --skipCacheInvalidation --env local",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:barrels": "barrelsby --delete -d ./migrations --singleQuotes",
    "e2e": "jest --config=jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=jest-e2e.json",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage"
  },
  "dependencies": {
    "@magishift/config": "^0.0.4",
    "@magishift/http": "^0.0.4",
    "@magishift/redis": "^0.0.5",
    "@magishift/util": "^0.0.3",
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.2",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.21.1",
    "barrelsby": "^2.1.1",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "midtrans-client": "^1.0.3",
    "mysql": "^2.17.1",
    "nestjs-swagger-api-implicit-queries-decorator": "^1.0.0",
    "pg": "^7.11.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/testing": "^6.3.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/supertest": "^2.0.7",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "serverless-domain-manager": "^3.2.6",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.3.3",
    "serverless-plugin-optimize": "^4.0.2-rc.1",
    "serverless-plugin-typescript": "^1.1.7",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.6",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/e2e/__mocks__/fileMock.js"
    }
  }
}